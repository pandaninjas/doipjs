{
  "plugins": ["plugins/markdown"],
  "source": {
    "include": [
        "./src",
        "./README.md"
    ]
  },
  "recurseDepth": 2,
  "templates": {
    "default": {
      "staticFiles": {
        "include": [
          "./static"
        ]
      }
    }
  },
  "opts": {
    "template": "node_modules/docdash"
  },
  "docdash": {
    "search": true
  }
}