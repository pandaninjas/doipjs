{
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-tsimport-plugin"
  ],
  "source": {
    "include": ["./src", "./README.md"]
  },
  "recurseDepth": 2,
  "templates": {
    "default": {
      "staticFiles": {
        "include": ["./static"]
      }
    }
  },
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "light",
      "menu": [
        {
          "title": "Source code",
          "link": "https://codeberg.org/keyoxide/doipjs",
          "target": "_blank"
        },
        {
          "title": "Keyoxide",
          "link": "https://keyoxide.org",
          "target": "_blank"
        }
      ]
    }
  }
}
